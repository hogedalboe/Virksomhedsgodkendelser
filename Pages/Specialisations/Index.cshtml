@page
@model Virksomhedsgodkendelser.Pages.Specialisations.IndexModel

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Specialisation[0].SpecialisationCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specialisation[0].SpecialisationName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specialisation[0].EducationCode)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Specialisation) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SpecialisationCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SpecialisationName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EducationCode)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
